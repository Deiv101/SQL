-- 1. 

SELECT yr, subject, winner
  FROM nobel
 WHERE yr = 1950;
 
 -- 2. 1962 Literature
 --    Show who won the 1962 prize for Literature.
 
 -- 3. Albert Einstein: Show the year and subject that won 'Albert Einstein' his prize.
 
 SELECT yr, subject
FROM nobel
WHERE winner = 'Albert Einstein';

 
