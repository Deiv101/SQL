    student
values
(2, 'Kate', 'Sociology');

insert into
    student
values
(3, 'Claire', 'English');

insert into
    student
values
(4, 'Jack', 'Biology');

insert into
    student
values
(5, 'Mike', 'Comp. Sci');

SELECT
    *
FROM
    student;
